{
  "name": "simp-tlog",
  "version": "1.0.0",
  "author": "simp",
  "summary": "A module for managing Tlog",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-tlog",
  "project_page": "https://github.com/simp/pupmod-simp-tlog",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "tlog",
    "logging",
    "security",
    "monitoring"
  ],
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 4.9.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 8.0.0 < 10.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/logrotate",
        "version_requirement": ">= 6.5.0 < 7.0.0"
